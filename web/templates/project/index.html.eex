<h2>Listing projects</h2>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Token</th>
      <th>Current coverage</th>
    </tr>
  </thead>
  <tbody>
<%= for project <- @projects do %>
    <tr>
      <td><%= link project.name, to: project_path(@conn, :show, project) %></a></td>
      <td><%= project.token %></td>
      <td><%= project.current_coverage %></td>
    </tr>
<% end %>
  </tbody>
</table>

<%= link "New project", to: project_path(@conn, :new) %>
